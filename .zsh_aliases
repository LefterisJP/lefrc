alias ls='ls --color'
alias gdbbt="gdb -q -n -ex 'thread apply all bt' -batch"

#emacs client in the terminal
alias ect='emacsclient -n --socket-name /tmp/emacs1000/server'
# gui emacs client
alias ecw='emacsclient -c --socket-name /tmp/emacs1000/server'

